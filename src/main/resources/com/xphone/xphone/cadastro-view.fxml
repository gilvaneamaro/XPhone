<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="1200.0" style="-fx-background-color: gray;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.xphone.xphone.CadastroController">
   <children>
      <VBox prefHeight="801.0" prefWidth="1200.0">
         <children>
            <MenuBar prefHeight="31.0" prefWidth="1201.0" style="-fx-background-color: black;">
               <menus>
                  <Menu mnemonicParsing="false" style="-fx-text-base-color: white; -fx-background-color: black; -fx-text-fill: #bc784e;" text="Cliente">
                     <items>
                        <MenuItem mnemonicParsing="false" style="-fx-text-fill: black;" text="Nova OS" />
                        <MenuItem mnemonicParsing="false" style="-fx-text-fill: black;" text="Consultar OS" />
                        <MenuItem mnemonicParsing="false" style="-fx-text-fill: black;" text="Buscar Clientes" />
                     </items>
                  </Menu>
                  <Menu mnemonicParsing="false" style="-fx-text-base-color: white;" text="Produtos">
                     <items>
                        <MenuItem mnemonicParsing="false" style="-fx-text-fill: black;" text="Consulta produto" />
                        <MenuItem mnemonicParsing="false" style="-fx-text-fill: black;" text="Gerenciar produtos" />
                     </items>
                  </Menu>
                  <Menu mnemonicParsing="false" style="-fx-text-base-color: white;" text="Caixa">
                     <items>
                        <MenuItem mnemonicParsing="false" style="-fx-text-fill: black;" text="Abrir caixa" />
                        <MenuItem mnemonicParsing="false" style="-fx-text-fill: black;" text="Lançar vendas" />
                        <MenuItem mnemonicParsing="false" style="-fx-text-fill: black;" text="Fechar caixa" />
                     </items>
                  </Menu>
                  <Menu mnemonicParsing="false" style="-fx-text-base-color: white;" text="Relatórios">
                     <items>
                        <MenuItem mnemonicParsing="false" style="-fx-text-fill: black;" text="Fechamento do dia" />
                        <MenuItem mnemonicParsing="false" style="-fx-text-fill: black;" text="Vendas por mes" />
                        <MenuItem mnemonicParsing="false" style="-fx-text-fill: black;" text="Venda por funcionario" />
                     </items>
                  </Menu>
               </menus>
            </MenuBar>
            <HBox prefHeight="82.0" prefWidth="1200.0">
               <children>
                  <Label prefHeight="51.0" prefWidth="412.0" text="Nova ordem de serviço" textFill="WHITE">
                     <HBox.margin>
                        <Insets left="30.0" right="-300.0" top="20.0" />
                     </HBox.margin>
                     <font>
                        <Font size="35.0" />
                     </font>
                  </Label>
                  <ImageView fitHeight="72.0" fitWidth="198.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../assets/logo-X-Phone%20amarelo.png" />
                     </image>
                     <HBox.margin>
                        <Insets left="800.0" top="20.0" />
                     </HBox.margin>
                  </ImageView>
               </children>
            </HBox>
            <HBox prefHeight="23.0" prefWidth="1200.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="147.0" text="CPF">
                     <HBox.margin>
                        <Insets left="30.0" />
                     </HBox.margin>
                  </Label>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
            <HBox prefHeight="37.0" prefWidth="1200.0">
               <children>
                  <TextField fx:id="cpf" prefHeight="25.0" prefWidth="147.0">
                     <HBox.margin>
                        <Insets left="30.0" />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
            <HBox prefHeight="22.0" prefWidth="1200.0">
