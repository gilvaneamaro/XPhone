                        <DatePicker fx:id="dataEntrega">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </DatePicker>
                        <Label prefHeight="24.0" prefWidth="75.0" text="OS aberta por">
                           <padding>
                              <Insets top="180.0" />
                           </padding>
                        </Label>
                        <ComboBox fx:id="funcionarioComboBox" prefWidth="150.0" />
                        <Button fx:id="criarOS" mnemonicParsing="false" onAction="#onCriarOSbutton" text="Criar OS">
                           <VBox.margin>
                              <Insets left="300.0" />
                           </VBox.margin>
                        </Button>
                     </children>
                     <HBox.margin>
                        <Insets left="30.0" />
                     </HBox.margin>
                  </VBox>
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0" />
         </children>
      </VBox>
