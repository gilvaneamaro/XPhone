            </HBox>
            <HBox prefHeight="46.0" prefWidth="1200.0">
               <children>
                  <TextField fx:id="nomeCliente" prefHeight="25.0" prefWidth="336.0">
                     <HBox.margin>
                        <Insets left="30.0" />
                     </HBox.margin>
                  </TextField>
                  <TextField fx:id="contatoCliente" prefHeight="25.0" prefWidth="147.0">
                     <HBox.margin>
                        <Insets left="30.0" />
                     </HBox.margin>
                  </TextField>
                  <TextField fx:id="emailCliente" prefHeight="25.0" prefWidth="205.0">
                     <HBox.margin>
                        <Insets left="30.0" />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
            <HBox prefHeight="22.0" prefWidth="1200.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="193.0" text="Marca">
                     <HBox.margin>
                        <Insets left="30.0" />
                     </HBox.margin>
                  </Label>
                  <Label prefHeight="17.0" prefWidth="136.0" text="Modelo" />
               </children>
            </HBox>
            <HBox prefHeight="46.0" prefWidth="1200.0">
               <children>
                  <TextField fx:id="marcaAparelho" prefHeight="25.0" prefWidth="160.0">
                     <HBox.margin>
                        <Insets left="30.0" />
                     </HBox.margin>
                  </TextField>
                  <TextField fx:id="modeloAparelho">
                     <HBox.margin>
                        <Insets left="30.0" />
                     </HBox.margin>
                  </TextField>
                  <CheckBox fx:id="liga" mnemonicParsing="false" text="Liga?">
                     <HBox.margin>
                        <Insets left="30.0" />
                     </HBox.margin>
                  </CheckBox>
                  <CheckBox fx:id="molhou" mnemonicParsing="false" text="Molhou?">
                     <HBox.margin>
                        <Insets left="30.0" />
                     </HBox.margin>
                  </CheckBox>
                  <CheckBox fx:id="telaQuebrada" mnemonicParsing="false" text="Tela quebrada?">
                     <HBox.margin>
                        <Insets left="30.0" />
                     </HBox.margin>
                  </CheckBox>
               </children>
            </HBox>
            <HBox prefHeight="26.0" prefWidth="1200.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="688.0" text="Descrição">
                     <HBox.margin>
                        <Insets left="30.0" />
                     </HBox.margin>
                  </Label>
                  <Label prefHeight="17.0" prefWidth="136.0" text="Previsão de entrega" />
               </children>
            </HBox>
            <HBox prefHeight="275.0" prefWidth="1224.0">
               <children>
                  <TextArea fx:id="descricaoOS" prefHeight="275.0" prefWidth="655.0">
                     <HBox.margin>
                        <Insets left="30.0" />
                     </HBox.margin>
                  </TextArea>
                  <VBox prefHeight="297.0" prefWidth="514.0">
